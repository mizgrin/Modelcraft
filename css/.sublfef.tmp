}
#featured-slider .slick-track:after {
 background: rgba(0, 0, 0, 0.16);
 content: "";
 height: 100%;
 position: absolute;
 top: 0;
 width: 100%;
 z-index: 1;
}
#featured-slider img{
  width: 100%;
  height: 100%;
}


.slick-prev:before, 
.slick-next:before {
  color: #fff !important;    
}


.reality-theme-main-slider .slick-slide img{
  width: 100%;
  height: auto;
  padding: 0;
}
.reality-theme-main-slider .slick-prev {
  left: -35px;
  -webkit-transition:all .4s ease-in-out;
  transition:all .4s ease-in-out
}
.reality-theme-main-slider .slick-next {
  right: -35px;
  -webkit-transition:all .4s ease-in-out;
  transition:all .4s ease-in-out
}
.reality-theme-main-slider:hover .slick-prev {
  left: 18px;
}
.reality-theme-main-slider:hover .slick-next{
  right: 18px;
}
.reality-theme-main-slider .slick-prev, 
.reality-theme-main-slider .slick-next{
  display: inline-block;
  zoom: 1;
  font-size: 28px;
  background:#39a84b;
  opacity: 1;
  font-size: 0;
  line-height: 0;
  position: absolute;
  top: 50%;
  display: block;
  width: 45px;
  height: 45px;
  padding: 0;
  z-index: 1;
  border-radius: 50%;
  -webkit-transform: translate(0, -50%);
  -ms-transform: translate(0, -50%);
  transform: translate(0, -50%);
  cursor: pointer;
  color: transparent;
  border: none;
  outline: none;
}
.reality-theme-main-slider .slick-prev:before,.reality-theme-main-slider  .slick-next:before{
 font-family: fontawesome;
 color: #fff;
 font-size: 28px;
 opacity: 1;
 line-height: 1;
}
.reality-theme-main-slider .slick-prev:before{
  content:"\f104" ;
  font-family: fontawesome;
}
.reality-theme-main-slider .slick-next:before {
  content: "\f105";
  font-family: fontawesome;
}

.reality-theme-main-slider .slick-prev:hover,
.reality-theme-main-slider .slick-prev:focus,
.reality-theme-main-slider .slick-next:hover,
.reality-theme-main-slider .slick-next:focus{
  color: #fff;
  outline: none;
  background: #f5562f;
}
.reality-theme-main-slider .slick-prev.slick-disabled:before, 
.reality-theme-main-slider .slick-next.slick-disabled:before{
  opacity: 0.8;
}
