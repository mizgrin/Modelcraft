}
.header-social li a {
    display: block;
    height: 25px;
    margin-right: 0;
    padding: 0;
    text-align: center;
    vertical-align: middle;
    width: 25px;
    line-height: 22px;
    border-radius: 50%;
    border:2px solid #fff;
}
.header-social ul,
.footer-social ul{
    margin: 0;
}
.header-social ul li,
.footer-social ul li{
  display: inline-block;
  margin-bottom: 0;
}
.header-social ul li{
    margin-right: 6px;
}
.header-social ul li a,
.footer-social ul li a{
    font-family: 'fontawesome';
    speak: none;
    content: "\f0c1";
    font-size: 13px; 
}

.header-social ul li a:after,
.footer-social ul li a:after{
    content: "\f0c1";
}

.header-social ul li a:after,
.footer-social ul li a:after{
    color: #fff;
}

.header-social ul li a[href*="facebook.com"]:after,
.footer-social ul li a[href*="facebook.com"]:after{
    content: "\f09a";
}
.header-social ul li a[href*="youtube.com"],
.header-social ul li a[href*="instagram.com"],
.header-social ul li a[href*="linkedin.com"],
.header-social ul li a[href*="twitter.com"],
.header-social ul li a[href*="facebook.com"]{
    background-color: transparent;
}
.footer-social ul li a[href*="facebook.com"]:hover:after{
    color: #4867AA;
}
.header-social ul li a[href*="facebook.com"]:hover{
    background-color: #4867AA;
}
.header-social ul li a[href*="twitter.com"]:after,
.footer-social ul li a[href*="twitter.com"]:after{
    content: "\f099";
}

.footer-social ul li a[href*="twitter.com"]:hover:after{
    color: #2383d0;
}
.header-social ul li a[href*="twitter.com"]:hover{
    background-color: #2383d0;
}

.header-social ul li a[href*="linkedin.com"]:after,
.footer-social ul li a[href*="linkedin.com"]:after{
    content: "\f0e1";
}

.footer-social ul li a[href*="linkedin.com"]:hover:after{
    color: #0077B5;
}
.header-social ul li a[href*="linkedin.com"]:hover{
    background-color: #0077B5;
}

.header-social ul li a[href*="instagram.com"]:after,
.footer-social ul li a[href*="instagram.com"]:after{
    content: "\f16d";
}

.footer-social ul li a[href*="instagram.com"]:hover:after{
    color: #BD3D87;
}
.header-social ul li a[href*="instagram.com"]:hover{
    background-color: #BD3D87;
}
.header-social ul li a[href*="youtube.com"]:after,
.footer-social ul li a[href*="youtube.com"]:after{
    content: "\f167";
}

.footer-social ul li a[href*="youtube.com"]:hover:after{
    color: #fb0202;
}
.header-social ul li a[href*="youtube.com"]:hover{
    background-color: #fb0202;
}
