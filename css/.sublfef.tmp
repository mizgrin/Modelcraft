.title-icon-holder.main-ico {
    height: 25px;
    width: 25px;
    font-size: 13px;
    background: #3aa84c ;
    color: #fff;    
    margin: 0px 15px 0px 0px;
}
.title-icon-holder  {
    box-shadow: #3aa84c 0px 0px 0px 2px inset;
    width: 15px;
    height: 15px;
    text-align: center;
    float: left;
    padding: 3px 2px;
    margin: 3px 20px 0px 5px;
    background: #fff;
    border-radius: 50%;
}
.iti-title-main {
    display: inline-block;
    float: left;
    font-size: 14px;
}
.itinerery-content.block,
.iti-content-expandable,
.itinerary-content-wrapper{
  position: relative;
}
.itinerery-content.block::before {
    content: "";
    position: absolute;
    top: 10px;
    bottom: 25px;
    left: 12px;
    border-left: 1px dashed rgba(6, 158, 193, 0.5490196078431373);
}
.iti-content-expandable .title{
  position: relative;
  font-weight: 600;
}
.single-iti-holder{
  position: relative;
  padding:15px 0;
}
.single-iti-holder:after{
    content: "";
    height: 1px;
    left: 39px;
    right: 0px;
    position: absolute;
    top: 100%;
    background: #ddd;
    margin: 0px;
}
.single-iti-holder:last-child:after{
    content: none;
}
.iti-content-expandable:after {
    content: '';
    right: 2px;
    top: 11px;
    position: absolute;
    width: 0;
    height: 0;
    border-style: solid;
    border-width: 5px 5px 0 5px;
    border-color:#000 transparent transparent transparent;
}
.iti-content-expandable[aria-expanded="true"]::after {
    border-width: 0px 5px 5px;
    border-color: transparent transparent #000;
}
.expanded-desc{
  text-align: justify;
  padding: 10px 0 5px 40px;
  font-size: 13px;
}
.iti-content-expandable:hover .title {
    color: #3aa84c;
}
.iti-title-main b{
  margin-right: 10px;
}
